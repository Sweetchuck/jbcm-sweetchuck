    <template name="fore"
              description="foreach(iterable_expr as $value) {...}"
              toReformat="true"
              toShortenFQNames="true">
        <variable name="iterable"
                  expression="phpVar"
                  defaultValue=""
                  alwaysStopAt="true" />
        <variable name="value"
                  expression="phpSuggestVariableName()"
                  defaultValue=""
                  alwaysStopAt="true" />
        <context>
            <option name="PHP Expression"
                    value="true" />
            <option name="PHP Statement"
                    value="true" />
        </context>
        <value>
<![CDATA[
foreach ($iterable$ as $value$) {
  $END$$SELECTION$
}
]]>
        </value>
