<templateSet group="Sweetchuck - Markdown">
    <template name="code"
              description="Code block"
              toReformat="false"
              toShortenFQNames="true">
        <variable name="language"
                  expression="enum(&quot;javascript&quot;, &quot;php&quot;)"
                  defaultValue=""
                  alwaysStopAt="true" />
        <context>
            <option name="ANY_OPENAPI_JSON_FILE"
                    value="false" />
            <option name="ANY_OPENAPI_YAML_FILE"
                    value="false" />
            <option name="CSS"
                    value="false" />
            <option name="CUCUMBER_FEATURE_FILE"
                    value="false" />
