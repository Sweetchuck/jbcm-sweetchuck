                    value="false" />
            <option name="JAVA_SCRIPT"
                    value="false" />
            <option name="JSON"
                    value="false" />
            <option name="OTHER"
                    value="true" />
            <option name="PHP"
                    value="false" />
            <option name="REQUEST"
                    value="false" />
            <option name="SHELL_SCRIPT"
                    value="false" />
            <option name="SQL"
                    value="false" />
