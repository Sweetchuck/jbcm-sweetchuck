                    value="false" />
            <option name="TypeScript"
                    value="false" />
            <option name="Vue"
                    value="false" />
            <option name="XML"
                    value="false" />
        </context>
        <value>
<![CDATA[
```$language$
$SELECTION$
```
]]>
        </value>
    </template>
